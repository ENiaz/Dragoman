ncit:C17557	rdf:type	owl:Class;
			rdfs:label	"Apoptosis";
			rdfs:subClassOf	genoschema:genoschema_0000;			
			dc:identifier "C17557" ;
			rdfs:comment "A form of programmed cell death that begins when a cell receives internal or external signals, then proceeds through a series of characteristic stages typically including rounding-up of the cell, retraction of pseudopods, reduction of cellular volume (pyknosis), chromatin condensation, nuclear fragmentation (karyorrhexis), and plasma membrane blebbing, and ends with the death of the cell.";
			dcterms:modified "2021-03-22"^^xsd:date;
			dcterms:creator <https://www.tib.eu/en/research-development/scientific-data-management/staff/samaneh-jozashoori/>.

