@prefix time: <http://www.w3.org/2006/time#> .
@base <http://genoschema.KG/> .

<http://genoschema.KG/Annotation>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/output/PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_001166/{diseaseId}";
    ];
    rr:predicateObjectMap [
        rr:predicate dce:1.1identifier;
        rr:objectMap [
        rml:reference "diseaseId"
        ]
    ].

<http://genoschema.KG/Publication>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/output/PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_000154/{pmid}";
        rr:class sio:SIO_000154
    ];
    rr:predicateObjectMap [
        rr:predicate dce:1.1identifier;
        rr:objectMap [
        rml:reference "pmid"
        ]
    ].

<http://genoschema.KG/Variation>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/output/PROJECT3.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://www.loria.fr/~coulet/ontology/snpontology/version1.6/snpontology_full.owl#dbsnp/{snpId}";
        rr:class vario:VariO_0001
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000061;
        rr:objectMap [
        rr:template  "http://semanticscience.org/resource/SIO_000899/{chromosome}"
        ]
    ].

<http://genoschema.KG/Evidence>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/output/PROJECT4.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://www.genoschema.com/genoschema_0018/{snpId}_{diseaseId}";
        rr:class genoschema:genoschema_0018
    ];
    rr:predicateObjectMap [
        rr:predicate :hasEnd;
        rr:objectMap [
        rr:template  "http://semanticscience.org/resource/SIO_000428/{YearFinal}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000628;
        rr:objectMap [
        rr:parentTriplesMap <http://genoschema.KG/Publication>;
        rr:joinCondition [
            rr:child "pmid";
            rr:parent "pmid";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate dcterms:provenance;
        rr:objectMap [
        rml:reference "source"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ru:curation_status;
        rr:objectMap [
        rml:reference "Confidence"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate af:afconfidence;
        rr:objectMap [
        rml:reference "score"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403;
        rr:objectMap [
        rr:parentTriplesMap <http://genoschema.KG/Variation>;
        rr:joinCondition [
            rr:child "snpId";
            rr:parent "snpId";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate :hasBeginning;
        rr:objectMap [
        rr:template  "http://semanticscience.org/resource/SIO_000428/{YearInitial}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403;
        rr:objectMap [
        rr:parentTriplesMap <http://genoschema.KG/Disease>;
        rr:joinCondition [
            rr:child "diseaseName";
            rr:parent "diseaseName";
        ];
        ]
    ].

<http://genoschema.KG/Disease>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/output/PROJECT5.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_010299/{diseaseName}";
        rr:class sio:SIO_010299
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000255;
        rr:objectMap [
        rr:parentTriplesMap <http://genoschema.KG/Annotation>;
        rr:joinCondition [
            rr:child "diseaseId";
            rr:parent "diseaseId";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
        rml:reference "diseaseName"
        ]
    ].

