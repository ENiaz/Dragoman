<http://genoschema.KG/TumorType>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/output/PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C159157/{Primary Tumor type}";
        rr:class ncit:C159157
    ].

<http://genoschema.KG/Variation>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/output/PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_011137/{cFormat}";
        rr:class sio:SIO_011137
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011133;
        rr:objectMap [
        rr:parentTriplesMap <replaceValue_output1>;
        rr:joinCondition [
            rr:child "replaceValue";
            rr:parent "replaceValue";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011134;
        rr:objectMap [
        rr:parentTriplesMap <replaceValue_output2>;
        rr:joinCondition [
            rr:child "individual_mutation";
            rr:parent "individual_mutation";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403;
        rr:objectMap [
        rr:parentTriplesMap <http://genoschema.KG/TumorType>;
        rr:joinCondition [
            rr:child "Primary Tumor type";
            rr:parent "Primary Tumor type";
        ];
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011134;
        rr:objectMap [
        rr:parentTriplesMap <concat3_output3>;
        rr:joinCondition [
            rr:child "Gene";
            rr:parent "Gene";
            ];
        rr:joinCondition [
            rr:child "replaceValue";
            rr:parent "replaceValue";
            ];
        ];
    ].

