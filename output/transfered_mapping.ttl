@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix ex: <http://www.example.com/> .
@base <http://GenoDIS.KG/> .

<#http://GenoDIS.KG/Evidence>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "../output/PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_011136/{evidence_id}";
        rr:class sio:resourceSIO_011136
    ];
    rr:predicateObjectMap [
        rr:predicate sio:resourceSIO_001403;
        rr:objectMap [
        rr:parentTriplesMap <http://GenoDIS.KG/Variation>
        ]
    ].

<#http://GenoDIS.KG/Variation>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "../output/PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_011137/{variant.clinical}";
        rr:class sio:resourceSIO_011137
    ];
    rr:predicateObjectMap [
        rr:predicate sio:resourceSIO_011134;
        rr:objectMap [
        rr:parentTriplesMap <#OUTPUT1>;
        rr:joinCondition [
            rr:child "hgvs_expressions";
            rr:parent "hgvs_expressions";
            ];
        rr:joinCondition [
            rr:child "gene.clinical";
            rr:parent "gene.clinical";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate sio:resourceSIO_011133;
        rr:objectMap [
        rr:parentTriplesMap <#OUTPUT2>;
        rr:joinCondition [
            rr:child "chromosome.clinical";
            rr:parent "chromosome.clinical";
            ];
        rr:joinCondition [
            rr:child "hgvs_expressions";
            rr:parent "hgvs_expressions";
            ];
        ];
    ].

<#OUTPUT1>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "../output/OUTPUT1.csv";
                rml:referenceFormulation ql:CSV
            ];
    rr:subjectMap [
        rml:reference "OUTPUT1"
    ].

<#OUTPUT2>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "../output/OUTPUT2.csv";
                rml:referenceFormulation ql:CSV
            ];
    rr:subjectMap [
        rml:reference "OUTPUT2"
    ].

