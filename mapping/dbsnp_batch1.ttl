<dbsnp1>
    rml:logicalSource [ rml:source "/data/sjozashoori/data-GenoDIS/data/dbsnp_batch6.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://purl.obolibrary.org/obo/VariO_0001/{ID}"; # rr:class snpo:dbsnp
        rr:class vario:VariO_0001 # variation
    ];                                                           
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000255; # has_annotation
        rr:objectMap <DNAannotation_resource1>; # genoschema:genoschema_00002
        rr:termType rr:IRI;
    ].

