@prefix ex: <http://www.example.com/> .
@base <http://genoschema.KG/> .

<Variation>
    rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/data/LC_LB_Seq.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://purl.obolibrary.org/obo/VariO_0001/{variantID}";
        rr:class vario:VariO_0001 # variation
    ];               
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000255; # has_annotation
        rr:objectMap <match_cds>; # genoschema:genoschema_00003
        rr:termType rr:IRI;
    ]. 

<match_cds>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/data/LC_LB_Seq.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:match_cdna 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:combinedValue;
            rr:objectMap [ 
                rml:reference "AAChange.refGene" 
            ]
        ];       
    ].


