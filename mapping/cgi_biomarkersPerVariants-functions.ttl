@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix genodis: <http://www.genodis.com/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix ex: <http://www.example.com/> .
@base <http://GenoDIS.KG/> .

<Evidence>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://www.genodis.com/genodis_0016/{Drug}_{Gene}_{cFormat}";
		rr:class genodis:genodis_0016 # Chemical-Gene-Variation-Association
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [
			rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Gene}"; # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <Drug>
		]
	]; 	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <Variation> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers_to 
		rr:objectMap [ 
			rr:parentTriplesMap <Publication>
		]
	];		
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000300; # has_value
		rr:objectMap [
			rml:reference "Association"
		]
	].			

<Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{cFormat}";
		rr:class sio:SIO_011137 # variation
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <TumorType>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011133; # has gDNA annotation
		rr:objectMap <cgi_gFormat>
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011134; # has cDNA annotation
		rr:objectMap <cgi_cFormat>
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011135; # has peptide annotation
		rr:objectMap <cgi_pFormat>
	].	

<Drug>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010038/{Drug}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038"; 
		]
	].	

<Transcript>	
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{transcript}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Gene}"; # gene
		]
	].

<Publication>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_000154/{PubMedID}"; 
		rr:class sio:SIO_000154; # publication
	].

<TumorType>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C159157/{Primary Tumor type}"; 
		rr:class ncit:C159157; # WHO AML CLassification of Tumors
	].

<cgi_gFormat>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/civic_Predictive.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:replaceValue 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
            	rml:reference "gDNA" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
            	rr:constant ":" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];        
    ].

<cgi_cFormat>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/civic_Predictive.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:concat3 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
            	rml:reference "Gene" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "cDNA" 
            ]
        ];        
    ]. 

<cgi_pFormat>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/civic_Predictive.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:replaceValue 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
            	rml:reference "individual_mutation" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
            	rr:constant ":" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];        
    ].

