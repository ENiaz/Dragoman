<Variation>
	rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/data/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap <pFormatReplaceFunction> ;

	rr:predicateObjectMap [
		rr:predicate rdf:type; # is_associated_with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_011137";
		]
	].

<pFormatReplaceFunction>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/Users/sam/Documents/GitHub/GenoDIS/data/CGI_biomarkersPerVariants.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:replaceValue 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
            	rml:reference "individual_mutation" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant ":" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
            	rr:constant "~p." 
            ]
        ];        
    ].