@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix genoschema: <http://www.genoschema.com/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix vario: <http://purl.obolibrary.org/obo/> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .
@prefix mesh: <http://purl.bioontology.org/ontology/MESH/> .
@prefix ex: <http://www.example.com/> .
@base <http://genoschema.KG/> .

<Evidence>
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://www.genoschema.com/genoschema_0016/{Drug}_{Gene}_{cFormat}";
		rr:class genoschema:genoschema_0016 # Chemical-Gene-Variation-Association
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [
			rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Gene}"; # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <Drug>
		]
	]; 	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <Variation> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers_to 
		rr:objectMap [ 
			rr:parentTriplesMap <Publication>
		]
	];		
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000300; # has_value
		rr:objectMap [
			rml:reference "Association"
		]
	].			

<Variation>
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{cFormat}";
		rr:class sio:SIO_011137 # variation
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <TumorType>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011133; # has_annotation
		rr:objectMap <gFormat_ReplaceFunction2> # genoschema:genoschema_00002
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011134; # has_annotation
		rr:objectMap <cFormat_ConcatFuncation> # genoschema:genoschema_00003
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011134; # has_annotation
		rr:objectMap <pFormatReplaceFunction> # genoschema:genoschema_00004
	].	

<Drug>
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010038/{Drug}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038"; 
		]
	].	

<Transcript>	
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{transcript}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Gene}"; # gene
		]
	].

<Publication>
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_000154/{PubMedID}"; 
		rr:class sio:SIO_000154; # publication
	].

<TumorType>
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C159157/{Primary Tumor type}"; 
		rr:class ncit:C159157; # WHO AML CLassification of Tumors
	].


