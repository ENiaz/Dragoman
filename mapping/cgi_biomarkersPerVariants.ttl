<gFormat_ReplaceFunction2>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:replaceValue 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap <gFormat_ReplaceFunction>;
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant ":" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];        
    ].
 
<gFormat_ReplaceFunction>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:replaceValue 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
            	rml:reference "gDNA" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "_" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];        
    ].

########

<cFormat_ConcatFuncation>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/CGI_biomarkersPerVariants.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:concat3 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
            	rml:reference "Gene" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap <cFormat_ReplaceFunction>
        ];        
    ]. 

