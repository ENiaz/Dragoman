@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms> .
@prefix dce: <http://purl.org/dc/elements/1.1> .
@prefix af: <http://purl.org/ontology/af> .
@prefix ru: <http://purl.org/imbi/ru-meta.owl#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix base: <http://semanticscience.org/resource> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix varharnom: <https://github.com/SDM-TIB/VariantHarmonizedNomenclature/blob/master/function.ttl#> .

<#Evidence>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011144/{Drug}_{Gene}_{cFormat}";
		rr:class sio:SIO_011144 # chemical-gene association linked with genomic alteration
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Gene}"; # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:parentTriplesMap <#Drug>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:parentTriplesMap <#Variation> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers to 
		rr:objectMap [ 
			rr:parentTriplesMap <#Publication>
		]
	];		
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000300; # has value
		rr:objectMap [
			rml:reference "Association"
		]
	].			

<#Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{cFormat}";
		rr:class sio:SIO_011137 # variation
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000255; # has annotation
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_011133/{gFormat}"; # gDNA variation notation (function is required)
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:parentTriplesMap <#TumorType>
		]
	].

<#Drug>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010038/{Drug}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038"; 
		]
	].	

<#Transcript>	
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{transcript}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Gene}"; # gene
		]
	].

<#Publication>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_000154/{PubMedID}"; 
		rr:class sio:SIO_000154; # publication
	].

<#TumorType>
	rml:logicalSource [ rml:source "/data/data/genomic/CGI_biomarkersPerVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl/{Primary Tumor type}"; 
		rr:class ncit:C159157; # WHO AML CLassification of Tumors
	].	
