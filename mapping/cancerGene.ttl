<Gene>  
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/CancerGenes.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Adaptive_immunity}"; # cancer gene
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C19540";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000001; # is_related_to
        rr:objectMap [
            rr:parentTriplesMap <CancerDriverGene>
