@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix genoschema: <http://www.genoschema.com/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix vario: <http://purl.obolibrary.org/obo/> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .
@prefix mesh: <http://purl.bioontology.org/ontology/MESH/> .
@prefix snpo: <http://www.loria.fr/~coulet/ontology/snpontology/version1.6/snpontology_full.owl#> .
@prefix ex: <http://www.example.com/> .
@prefix stato: <http://purl.obolibrary.org/obo/> .
@base <http://genoschema.KG/> .

<Patient>   
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/patient_variant_sample.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_000393/{patientID}";
        rr:class sio:SIO_000393 # patient
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:parentTriplesMap <Variation>
        ]
    ].   

<Variation>
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/patient_variant_sample.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap <AAannotationFunction> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://purl.obolibrary.org/obo/VariO_0001";     
        ]
    ].

##########################################################################################
######################################## Functions #######################################
##########################################################################################

<AAannotationFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/patient_variant_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:concat4 
            ]
        ]; 
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
                rr:constant "http://purl.obolibrary.org/obo/VariO_0001/" 
            ]
        ];                 
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rml:reference "Gene.refGene" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
                rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value4;
            rr:objectMap <match_aa>;
        ];        
    ]. 

<match_aa>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/patient_variant_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:match_aa 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:combinedValue;
            rr:objectMap [ 
                rml:reference "AAChange.refGene" 
            ]
        ];       
    ]. 
