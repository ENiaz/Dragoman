@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix genoschema: <http://www.genoschema.com/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix vario: <http://purl.obolibrary.org/obo/> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .
@prefix mesh: <http://purl.bioontology.org/ontology/MESH/> .
@prefix ex: <http://www.example.com/> .
@base <http://genoschema.KG/> .

<Variation>
	rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/OncoKB_allAnnotatedVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://purl.obolibrary.org/obo/VariO_0001/{v_ID}"; #generated identifier
		rr:class vario:VariO_0001 # variation 
	];		
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is_located_in
		rr:objectMap [
			rr:parentTriplesMap <Transcript>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is_located_in
		rr:objectMap [
			rr:parentTriplesMap <Gene>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://www.genoschema.com/genoschema_0014/{Mutation Effect}"; # Variation Impact
		]
	];		
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://www.genoschema.com/genoschema_0017/{Oncogenicity}"; # Oncogenicity
		]
	].	

<AA_annotation>
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/OncoKB_allAnnotatedVariants.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap <pFormatFunction> ;
    rr:termType rr:IRI ;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://www.genoschema.com/genoschema_0001";
        ]
    ]. 
    
<Transcript>	
	rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/OncoKB_allAnnotatedVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{Isoform}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:parentTriplesMap <Gene>
		]
	].	

<Gene>	
	rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/OncoKB_allAnnotatedVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Hugo Symbol}";
		rr:class ncit:C16612 # Gene
	].

#############

<pFormatFunction>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/OncoKB_allAnnotatedVariants.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:concat4 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
                rr:constant "http://www.genoschema.com/genoschema_0004/" 
            ]
        ];        
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rml:reference "Hugo Symbol" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
            	rr:constant "~p." 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value4;
            rr:objectMap [ 
            	rml:reference "Protein Change" 
            ]
        ];        
    ].	