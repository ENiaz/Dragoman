<DNAannotation_resource>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/germline_tp53.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:concat2 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
                rr:constant "http://www.genoschema.com/genoschema_0002/TP53~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap <DNAannotationFunction>;
        ];               
    ].

<DNAannotationFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/germline_tp53.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:replaceValue 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
                rml:reference "g_description" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rr:constant ">" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
                rr:constant "~" 
            ]
        ];                            
    ]. 


