<Variation>
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/germline_tp53.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://purl.obolibrary.org/obo/VariO_0001/{v_ID}";
        rr:class vario:VariO_0001 # variation
    ];
    rr:predicateObjectMap [
        rr:predicate genoschema:genoschema_0014; #variantImpact
        rr:objectMap [
            rml:reference "Effect";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate genoschema:genoschema_0023; # functionalImpcat
        rr:objectMap [
            rml:reference "SIFTClass";
        ]
    ].

<DNA_annotation>
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/germline_tp53.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap <DNAannotation_resource> ;
    rr:termType rr:IRI ;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://www.genoschema.com/genoschema_0001";
        ]
    ]. 

