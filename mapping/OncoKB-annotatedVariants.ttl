@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix ex: <http://www.example.com/> .
@base <http://GenoDIS.KG/> .

<Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/OncoKB_allAnnotatedVariants.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{pFormat}";
		rr:class sio:SIO_011137 # variation
	];		
#	rr:predicateObjectMap [
#		rr:predicate iasis:proteinChange;
#		rr:objectMap [
#			rml:reference "Protein Change"
#		]
#	];				
#	rr:predicateObjectMap [
#		rr:predicate iasis:variant_oncogenicity;
#		rr:objectMap [
#			rml:reference "Oncogenicity"
#		]
#	];
#	rr:predicateObjectMap [
#		rr:predicate iasis:mutation_effect;
#		rr:objectMap [
#			rml:reference "Mutation Effect"
#		]
#	];
	rr:predicateObjectMap [
		rr:predicate iasis:variation_isLocatedIn_transcript;
		rr:objectMap [
			rr:parentTriplesMap <Transcript>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011135; # has peptide annotation
		rr:objectMap <oncokb_pFormat>
	].		

<Transcript>	
	rml:logicalSource [ rml:source "/data/data/genomic/OncoKB_allAnnotatedVariants.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{Isoform}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Hugo Symbol}"; # gene
		]
	].	

<oncokb_pFormat>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/OncoKB_allAnnotatedVariants.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:concat3 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
            	rml:reference "Hugo Symbol" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "Protein Change" 
            ]
        ];        
    ].	