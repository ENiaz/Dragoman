@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix ex: <http://www.example.com/> .
@base <http://GenoDIS.KG/> .

<Evidence>
    rml:logicalSource [ rml:source "/Users/maria-esthervidal/Documents/docker/GenoDIS/data/civic_Predictive.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_011136/{evidence_id}";
        rr:class sio:SIO_011136 # chemical-disease association linked with genomic alteration
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is associated with
        rr:objectMap [ 
            rr:parentTriplesMap <Variation> 
        ]
    ].      
    
<Variation>
    rml:logicalSource [ rml:source "/Users/maria-esthervidal/Documents/docker/GenoDIS/data/civic_Predictive.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_011137/{variant.clinical}";
        rr:class sio:SIO_011137 # variation
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011133; # has gDNA annotation
        rr:objectMap <civic_gFormat>
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011134; # has cDNA annotation
        rr:objectMap <civic_cFormat>
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011135; # has peptide annotation
        rr:objectMap <civic_pFormat>
    ].

<civic_gFormat>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/Users/maria-esthervidal/Documents/docker/GenoDIS/data/civic_Predictive.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:civic_gFormat 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:hgvs;
            rr:objectMap [ 
                rml:reference "hgvs_expressions" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:chromosome;
            rr:objectMap [ 
                rml:reference "chromosome.clinical" 
            ]
        ];
    ].          

<civic_cFormat>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/Users/maria-esthervidal/Documents/docker/GenoDIS/data/civic_Predictive.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:civic_cFormat 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:hgvs;
            rr:objectMap [ 
                rml:reference "hgvs_expressions" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:gene;
            rr:objectMap [ 
                rml:reference "gene.clinical" 
            ]
        ];
    ].      

<civic_pFormat>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/Users/maria-esthervidal/Documents/docker/GenoDIS/data/civic_Predictive.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:civic_pFormat 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:threeLetters;
            rr:objectMap <civic_aa_threeLetters> 
        ];    
    ].

<civic_aa_threeLetters>
