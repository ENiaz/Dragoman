@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix genodis: <http://www.genodis.com/> .
@prefix ex: <http://www.example.com/> .
@base <http://GenoDIS.KG/> .

<Transcript>	
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{Accession Number}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Gene name}" # gene
		]
	].

<Gene>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010035/{Gene name}"; # gene
		rr:class sio:SIO_010035
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:parentTriplesMap <Sample> 
		]
	].	

<Sample>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_001050/{ID_sample}";
		rr:class sio:SIO_001050 # sample
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000068; # is part of
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010002"; # tissue
		]
	].

<Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{variant.clinical}";
		rr:class sio:SIO_011137 # variation
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is located in
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{gene.clinical}" # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000001; # is related to
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_000136/{Mutation somatic status}" # Somatic status description
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is located in
		rr:objectMap [
			rr:parentTriplesMap <Transcript>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is located in
		rr:objectMap [
			rr:parentTriplesMap <Sample>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers to
		rr:objectMap [ 
			rr:parentTriplesMap <Publication>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011134; # has cDNA annotation
		rr:objectMap <cFormat>
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011135; # has peptide annotation
		rr:objectMap <pFormat>
	].

<cFormat>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:concat3 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
			rr:objectMap <cFormat_replaceRegex>
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "Mutation CDS" 
            ]
        ];        
    ].    

<cFormat_replaceRegex>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:replaceRegex 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:regex;
            rr:objectMap [ 
                rr:constant "_.*" 
            ] 
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:replvalue;
            rr:objectMap [ 
                rr:constant "" 
            ] 
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:column;
            rr:objectMap [ 
                rml:reference "Gene name" 
            ] 
        ];                     
    ].

<pFormat>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:concat3 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
			rr:objectMap <pFormat_replaceRegex>
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
			rr:objectMap <pFormat_split>
        ];     
    ].  

<pFormat_replaceRegex>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:replaceRegex 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:regex;
            rr:objectMap [ 
                rr:constant "_.*" 
            ] 
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:replvalue;
            rr:objectMap [ 
                rr:constant "" 
            ] 
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:column;
            rr:objectMap [ 
                rml:reference "Gene name" 
            ] 
        ];                     
    ].

<pFormat_split>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:split 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:column;
            rr:objectMap [ 
                rml:reference "Mutation AA" 
            ] 
        ]; 
        rr:predicateObjectMap [
            rr:predicate ex:column;
            rr:objectMap [ 
                rml:reference "value" 
            ] 
        ];           
        rr:predicateObjectMap [
            rr:predicate ex:index;
            rr:objectMap [ 
                rr:constant "1" 
            ] 
        ];                    
    ].

<Publication>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_000154/{citation_id}"; 
		rr:class sio:SIO_000154; # publication
	].
