@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix ex: <http://www.example.com/> .
@base <http://GenoDIS.KG/> .

<Transcript>	
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{Accession Number}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Gene name}" # gene
		]
	].

<Gene>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010035/{Gene name}"; # gene
		rr:class sio:SIO_010035
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:parentTriplesMap <Sample> 
		]
	].	

<Sample>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_001050/{ID_sample}";
		rr:class sio:SIO_001050 # sample
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000068; # is part of
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010002"; # tissue
		]
	].

<Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{variant.clinical}";
		rr:class sio:SIO_011137 # variation
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is located in
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{gene.clinical}" # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000001; # is related to
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_000136/{Mutation somatic status}" # Somatic status description
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is located in
		rr:objectMap [
			rr:parentTriplesMap <Transcript>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is located in
		rr:objectMap [
			rr:parentTriplesMap <Sample>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers to
		rr:objectMap [ 
			rr:parentTriplesMap <Publication>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011133; # has gDNA annotation
		rr:objectMap <civic_gFormat>
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011134; # has cDNA annotation
		rr:objectMap <civic_cFormat>
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011135; # has peptide annotation
		rr:objectMap <civic_pFormat>
	].

<civic_cFormat>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex: 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:;
            rr:objectMap [ 
                rml:reference "" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:;
            rr:objectMap [ 
                rml:reference "" 
            ]
        ];
    ].      

<civic_pFormat>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex: 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:;
            rr:objectMap [ 
                rml:reference "" 
            ] 
        ];    
    ].

<Publication>
	rml:logicalSource [ rml:source "/data/data/genomic/cosmic_mutation_sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_000154/{citation_id}"; 
		rr:class sio:SIO_000154; # publication
	].
