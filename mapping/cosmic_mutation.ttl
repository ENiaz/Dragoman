@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix genoschema: <http://www.genoschema.com/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix vario: <http://purl.obolibrary.org/obo/> .
@prefix ex: <http://www.example.com/> .
@base <http://genoschema.KG/> .

<Transcript>    
    rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_010017/{Accession Number}";
        rr:class sio:SIO_010017 # RNA
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Gene name}" # gene
        ]
    ].

<Gene>
    rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Gene name}"; # gene
        rr:class ncit:C16612
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [ 
            rr:parentTriplesMap <Sample> 
        ]
    ].  

<Sample>
    rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_001050/{ID_sample}";
        rr:class sio:SIO_001050 # sample
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000068; # is_part_of
        rr:objectMap [ 
            rr:template "http://semanticscience.org/resource/SIO_010002/{Primary site}"; # tissue
        ]
    ].

<Variation>
    rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://purl.obolibrary.org/obo/VariO_0001/{MUTATION_ID}";
        rr:class vario:VariO_0001 # variation
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000061; # is_located_in
        rr:objectMap [
            rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Gene name}" # gene
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000001; # is_related_to
        rr:objectMap [
            rr:template "http://semanticscience.org/resource/SIO_000136/{Mutation somatic status}" # Somatic status description
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000061;  # is_located_in
        rr:objectMap [
            rr:parentTriplesMap <Transcript>
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000061;  # is_located_in
        rr:objectMap [
            rr:parentTriplesMap <Sample>
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000628; # refers_to
        rr:objectMap [ 
            rr:parentTriplesMap <Publication>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011134; # has_annotation
        rr:objectMap <cFormatFunction> # genoschema:genoschema_00003
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_011135; # has_annotation
        rr:objectMap <pFormatFunction> # genoschema:genoschema_00004
    ].

<Publication>
    rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_000154/{Pubmed_PMID}"; 
        rr:class sio:SIO_000154; # publication
    ].

##########################################################################################
######################################## Functions #######################################
##########################################################################################


<cFormatFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:concat3 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap <GeneName_ReplaceRegexFunction>
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
                rml:reference "Mutation CDS" 
            ]
        ];        
    ].    

<GeneName_ReplaceRegexFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:replaceRegex 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:regex;
            rr:objectMap [ 
                rr:constant "_.*" 
            ] 
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:replvalue;
            rr:objectMap [ 
                rr:constant "" 
            ] 
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
                rml:reference "Gene name" 
            ] 
        ];                     
    ].

<pFormatFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/genoschema-master/data/cosmic_mutation_sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:concat3 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap <GeneName_ReplaceRegexFunction>
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
                rml:reference "Mutation AA" 
            ] 
        ];    
    ].   
