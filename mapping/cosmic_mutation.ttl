@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix ex: <http://www.example.com/> .
@base <http://GenoDIS.KG/> .

<Transcript>	
	rml:logicalSource [ rml:source "/data/data/genomic/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{Accession Number}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{Gene name}" # gene
		]
	].

<Gene>
	rml:logicalSource [ rml:source "/data/data/genomic/COSMIC_mutation_grch37.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010035/{Gene name}"; # gene
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010035" 
		]
	].

<Sample>
	rml:logicalSource [ rml:source "/data/data/genomic/COSMIC_mutation_grch37.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Sample/{ID_sample}";
		rr:class sio:Sample
	];
	rr:predicateObjectMap [
		rr:predicate sio:sampleLabel;
		rr:objectMap [
			rml:reference "Sample name"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:sampleID;
		rr:objectMap [
			rml:reference "ID_sample"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:sampleAge;
		rr:objectMap [ 
			rml:reference "Age" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:primaryTissue;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Tissue/{Primary site}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:tissueSubtype1;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SubTissue/{Site subtype 1}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:primaryHistology;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/TissueType/{Primary histology}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:histologySubtype1;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/TissueSubType/{Histology subtype 1}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:sample_isTakenFrom_tumor;
		rr:objectMap [ 
			rr:parentTriplesMap <Tumor>
		]
	].

<Tumor>
	rml:logicalSource [ rml:source "/data/data/genomic/COSMIC_mutation_grch37.csv";
						rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Tumor/{ID_tumour}";
		rr:class sio:Tumor;
	];
	rr:predicateObjectMap [
		rr:predicate sio:tumorID;
		rr:objectMap [
			rml:reference "ID_tumour"
		]
	].

<Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{variant.clinical}";
		rr:class sio:SIO_011137 # variation
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is located in
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_000899/{chromosome.clinical}" # chromosome
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is located in
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{gene.clinical}" # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is located in
		rr:objectMap [
			rr:parentTriplesMap <Transcript>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001279; # has phenotype
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010056/{phenotypes}" # phenotype 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers to
		rr:objectMap [ 
			rr:parentTriplesMap <Publication>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011133; # has gDNA annotation
		rr:objectMap <civic_gFormat>
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011134; # has cDNA annotation
		rr:objectMap <civic_cFormat>
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_011135; # has peptide annotation
		rr:objectMap <civic_pFormat>
	].


<Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/COSMIC_mutation_grch37.csv";
						rml:referenceFormulation ql:CSV ];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Variation/{cFormat}";
		rr:class sio:Variation;
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/vocab/CDNA-Variation"
		]
	];	
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:parentTriplesMap <#Peptide-Variation>
		]
	];
			
	rr:predicateObjectMap [
		rr:predicate sio:zygosity;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Zygosity/{Mutation zygosity}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:loh;
		rr:objectMap [ 
			rml:reference "LOH"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:snp;
		rr:objectMap [ 
			rml:reference "SNP"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variation_aa;
		rr:objectMap [ 
			rml:reference "Mutation AA"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variation_cds;
		rr:objectMap [ 
			rml:reference "Mutation CDS"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:fathmm_prediction;
		rr:objectMap [ 
			rml:reference "FATHMM prediction"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:fathmm_score;
		rr:objectMap [ 
			rml:reference "FATHMM score" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variation_somatic_status;
		rr:objectMap [ 
			rml:reference "Mutation somatic status" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variationDescription;
		rr:objectMap [
			rml:reference "Mutation Description"
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variation_genome_position;
		rr:objectMap [ 
			rml:reference "Mutation genome position" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variation_strand;
		rr:objectMap [ 
			rml:reference "Mutation strand" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variation_studyID;
		rr:objectMap [ 
			rml:reference "ID_STUDY" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:variation_isLocatedIn_sample;
		rr:objectMap [ 
			rr:parentTriplesMap <#Sample> 
		]
	];


<Peptide-Variation>
	rml:logicalSource [ rml:source "/data/data/genomic/COSMIC_mutation_grch37.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Variation/{pFormat}";
		rr:class sio:Peptide-Variation
	].

<Publication>
	rml:logicalSource [ rml:source "/data/data/genomic/COSMIC_mutation_grch37.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Publication/{Pubmed_PMID}";
		rr:class sio:Publication;
	];
	rr:predicateObjectMap [
		rr:predicate sio:pubmedID;
		rr:objectMap [ 
			rml:reference "Pubmed_PMID" 
		]
	].
