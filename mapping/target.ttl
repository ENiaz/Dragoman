@base <http://genoschema.KG/> .

<Target>
	rml:logicalSource [ rml:source "/mnt/e/data/data/drug/target.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010423/{Target_name}";
		rr:class sio:SIO_010423
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://purl.obolibrary.org/obo/CHEBI_36080/{Uni_ID}"; ## protein
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:externalLink;
		rr:objectMap [
			rr:template "https://www.uniprot.org/uniprot/{Uni_ID}";
		]
	];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Gene_name}" # gene
        ]
    ].
