<DrugTargetInteraction>
	rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/drugTargetInteraction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010013/{Drug}_interactsWith_{Protein ID}";
		rr:class sio:SIO_010013 # ChemicalInteraction
	];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000001; # is_related_to
        rr:objectMap [ 
            rr:template "http://semanticscience.org/resource/SIO_010038/{Drug}" # Drug
        ]
    ];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000001; #is_related_to
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010423/{Protein ID}" # Target
		]
	].
