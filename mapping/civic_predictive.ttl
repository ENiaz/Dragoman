<gFormat_concat>
	fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/civic_Predictive.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:concat4 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
            	rr:constant "chr" 
            ]
        ];        
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rml:reference "chromosome.clinical" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
            	rr:constant "~" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap <gFormat_split>
        ];        
    ].         

<gFormat_split>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/data/genomic/civic_Predictive.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:split 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:column;
            rr:objectMap <gFormat> 
        ];
        rr:predicateObjectMap [
            rr:predicate ex:separator;
            rr:objectMap [ 
                rr:constant "," 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
            	rr:constant "0" 
            ]
        ];        
    ].  

