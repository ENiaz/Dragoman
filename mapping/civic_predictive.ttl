<Evidence>
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://www.genoschema.com/genoschema_0015/{evidence_id}";
		rr:class genoschema:genoschema_0015 # Chemical-Disease-Variation-Association
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <Disease> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers_to 
		rr:objectMap [ 
			rr:parentTriplesMap <Publication>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:parentTriplesMap <Variation> 
		]
	].		
	
<Variation>
	rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://purl.obolibrary.org/obo/VariO_0001/{variant.clinical}";
		rr:class vario:VariO_0001 # variation
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug1}" # drug
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug2}" # drug
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug3}" # drug
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug4}" # drug
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is_associated_with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug5}" # drug
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is_located_in
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_000899/{chromosome.clinical}" # chromosome
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is_located_in
		rr:objectMap [
			rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{gene.clinical}" # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is_located_in
		rr:objectMap [
			rr:parentTriplesMap <Transcript>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001279; # has_phenotype
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010056/{phenotypes}" # phenotype 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers_to
		rr:objectMap [ 
			rr:parentTriplesMap <Publication>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000255; # has_annotation
		rr:objectMap <gFormat_concat>; # genoschema:genoschema_0002
        rr:termType rr:IRI;
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000255; # has_annotation
		rr:objectMap <cFormat_concat>; # genoschema:genoschema_0003
        rr:termType rr:IRI;
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000255; # has_annotation
		rr:objectMap <match_pFormat>; # genoschema:genoschema_0004
        rr:termType rr:IRI;
	].

<Disease>
    rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/civic_Predictive.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_010299/{disease}"; 
        rr:class sio:SIO_010299 # disease
    ].

<Transcript>    
    rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/civic_Predictive.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_010017/{representative_transcript.clinical}";
        rr:class sio:SIO_010017 # RNA
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{gene.clinical}" # gene
        ]
    ].

<Publication>
    rml:logicalSource [ rml:source "/mnt/e/data-GenoKGC/data/civic_Predictive.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_000154/{citation_id}"; 
        rr:class sio:SIO_000154; # publication
    ].

