@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix genoschema: <http://www.genoschema.com/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix vario: <http://purl.obolibrary.org/obo/> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .
@prefix mesh: <http://purl.bioontology.org/ontology/MESH/> .
@prefix snpo: <http://www.loria.fr/~coulet/ontology/snpontology/version1.6/snpontology_full.owl#> .
@prefix ex: <http://www.example.com/> .
@base <http://genoschema.KG/> .

<Variation>
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://purl.obolibrary.org/obo/VariO_0001/{variantID}";
        rr:class vario:VariO_0001 # variation
    ];  
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000061; #is_located_in
        rr:objectMap <ExonFunction>
    ];   
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; #is_associated_with
        rr:objectMap <VariationImpact>
    ];          
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; #is_associated_with
        rr:objectMap <PopulationAF>
    ];                              
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; #is_associated_with
        rr:objectMap [
            rml:reference "AF";
            rr:datatype xsd:float;
        ]
    ]; 
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:parentTriplesMap <Gene>
        ]
    ];              
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:parentTriplesMap <VariantType>
        ]
    ];      
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; #is_associated_with
        rr:objectMap [
            rr:parentTriplesMap <Patient>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:parentTriplesMap <LiquidBiopsy>
        ]
    ];              
    rr:predicateObjectMap [
        rr:predicate skos:exactMatch;
        rr:objectMap [
            rr:template "http://purl.obolibrary.org/obo/VariO_0001/{snp138}";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000255; # has_annotation
        rr:objectMap <DNAannotation_resource>; # genoschema:genoschema_00002
        rr:termType rr:IRI;
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000255; # has_annotation
        rr:objectMap <CDSannotationFunction>; # genoschema:genoschema_00003
        rr:termType rr:IRI;
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000255; # has_annotation
        rr:objectMap <AAannotationFunction>; # genoschema:genoschema_00004
        rr:termType rr:IRI;
    ].  

<VariantType>  
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://www.genoschema.com/genoschema_0021/{variantType_basedOnAF}"; 
        rr:class genoschema:genoschema_0021
    ].

<Gene>  
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16612/{Gene.refGene}"; # gene
        rr:class ncit:C16612
    ].

<VariationImpact>  
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://www.genoschema.com/genoschema_0014/{ExonicFunc.refGene}"
        rr:class genoschema:genoschema_0014
    ].

<PopulationAF>  
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://www.genoschema.com/genoschema_0012/{gnomAD_genome_ALL}"
        rr:class genoschema:genoschema_0012
    ].

<Patient>   
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://semanticscience.org/resource/SIO_000393/{patientID}";
        rr:class sio:SIO_000393 # patient
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_001403; # is_associated_with
        rr:objectMap [
            rr:parentTriplesMap <LiquidBiopsy>
        ]
    ].

<LiquidBiopsy>  
    rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap [
        rr:template "http://purl.bioontology.org/ontology/MESH/{sampleID}";
        rr:class mesh:D000073890 # liquid biopsy
    ];
    rr:predicateObjectMap [
        rr:predicate dcterms:date;
        rr:objectMap [
            rml:reference "sampleExtractionDate";
            rr:datatype xsd:date;
        ]
    ].  


##########################################################################################
######################################## Functions #######################################
##########################################################################################

<ExonFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:match_exon 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:combinedValue;
            rr:objectMap [ 
                rml:reference "AAChange.refGene" 
            ]
        ];       
    ]. 

########
<DNAannotation_resource>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:concat2 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
                rr:constant "http://www.genoschema.com/genoschema_0002/" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap <DNAannotationFunction>;
        ];               
    ].

<DNAannotationFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:concat6 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate ex:value1;
            rr:objectMap [ 
                rml:reference "Chr" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rr:constant "~g." 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap <PositionSplitFunction> 
        ]; 
        rr:predicateObjectMap [
            rr:predicate ex:value4;
            rr:objectMap [ 
                rml:reference "Ref" 
            ]
        ]; 
        rr:predicateObjectMap [
            rr:predicate ex:value5;
            rr:objectMap [ 
                rr:constant "~" 
            ]
        ]; 
        rr:predicateObjectMap [
            rr:predicate ex:value6;
            rr:objectMap [ 
                rml:reference "Alt" 
            ]
        ];                              
    ]. 

<PositionSplitFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/sjozashoori/GenoKGC/data/LC_SB_Seq.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:split 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:column;
            rr:objectMap [ 
                rml:reference "Start" 
            ]
        ]; 
        rr:predicateObjectMap [
            rr:predicate ex:separator;
            rr:objectMap [ 
                rr:constant "." 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:index;
            rr:objectMap [ 
                rr:constant "0" 
            ]
        ];   
    ].

